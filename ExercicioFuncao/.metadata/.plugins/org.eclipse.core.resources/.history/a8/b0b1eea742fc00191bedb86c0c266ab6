import java.util.*;
public class Main {
	public static void main(String[] args) {
		Scanner in = new Scanner (System.in);
		
		double n;
		int pos = 0;
		
		for (int i = 1; i < 7; i++) {
			n = in.nextDouble();
			if(n > 0) {
				pos++;
			}
		}
		
		System.out.println(pos+" valores positivos");
		
		
		
	}

}
